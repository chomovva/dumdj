.jumbotron#jumbotron.jquery-background-video-wrapper
	video.bg-video.jquery-background-video( loop autoplay muted playsinline poster=src.video+jumbotron_video_name+".jpg" )
		source( src=src.video+jumbotron_video_name+".mp4" type="video/mp4" )
		source( src=src.video+jumbotron_video_name+".webm" type="video/webm" )
		source( src=src.video+jumbotron_video_name+".ogv" type="video/ogg" )
	.wrap
		h1.title #{page.title}
		p.subtitle #{blog.description}
		a.permalink( href="#" ) О нас